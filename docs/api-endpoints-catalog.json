          "requiresAuth": true,
          "requestBody": {
            "status": "pending|accepted|ready|picked_up|in_transit|delivered|cancelled"
          },
          "curl": "curl -X PUT https://api.brillprime.com/api/orders/ORDER_ID -H 'Authorization: Bearer YOUR_TOKEN' -H 'Content-Type: application/json' -d '{\"status\":\"accepted\"}'"
