    "authentication": {
      "description": "User authentication and profile management",
      "endpoints": [
        {
          "method": "POST",
          "path": "/api/auth/register",
          "description": "Register a new user",
          "requiresAuth": false,
          "requestBody": {
            "email": "string",
            "password": "string",
            "firstName": "string",
            "lastName": "string",
            "phone": "string",
            "role": "consumer|merchant|driver",
            "firebaseUid": "string"
          },
          "curl": "curl -X POST https://api.brillprime.com/api/auth/register -H 'Content-Type: application/json' -d '{\"email\":\"test@example.com\",\"password\":\"Test123!\",\"firstName\":\"John\",\"lastName\":\"Doe\",\"phone\":\"+2348012345678\",\"role\":\"consumer\",\"firebaseUid\":\"firebase123\"}'"
        },
        {
          "method": "POST",
          "path": "/api/auth/login",
          "description": "User login",
          "requiresAuth": false,
          "requestBody": {
            "email": "string",
            "firebaseUid": "string"
          },
          "curl": "curl -X POST https://api.brillprime.com/api/auth/login -H 'Content-Type: application/json' -d '{\"email\":\"test@example.com\",\"firebaseUid\":\"firebase123\"}'"
        },
        {
          "method": "POST",
          "path": "/api/auth/social-login",
          "description": "Social media login (Google, Apple, Facebook)",
          "requiresAuth": false,
          "requestBody": {
            "provider": "google|apple|facebook",
            "firebaseUid": "string",
            "email": "string",
            "fullName": "string",
            "photoUrl": "string",
            "role": "consumer|merchant|driver"
          },
          "curl": "curl -X POST https://api.brillprime.com/api/auth/social-login -H 'Content-Type: application/json' -d '{\"provider\":\"google\",\"firebaseUid\":\"firebase123\",\"email\":\"test@example.com\",\"fullName\":\"John Doe\",\"photoUrl\":\"https://example.com/photo.jpg\",\"role\":\"consumer\"}'"
        },
        {
          "method": "GET",
          "path": "/api/auth/profile",
          "description": "Get user profile",
          "requiresAuth": true,
          "curl": "curl -X GET https://api.brillprime.com/api/auth/profile -H 'Authorization: Bearer YOUR_TOKEN'"
        },
        {
          "method": "PUT",
          "path": "/api/auth/profile",
          "description": "Update user profile",
          "requiresAuth": true,
          "requestBody": {
            "firstName": "string",
            "lastName": "string",
            "email": "string",
            "phone": "string",
            "address": "string"
          },
          "curl": "curl -X PUT https://api.brillprime.com/api/auth/profile -H 'Authorization: Bearer YOUR_TOKEN' -H 'Content-Type: application/json' -d '{\"firstName\":\"John\",\"lastName\":\"Doe\",\"phone\":\"+2348012345678\"}'"
        },
        {
          "method": "PUT",
          "path": "/api/auth/deactivate",
          "description": "Deactivate user account",
          "requiresAuth": true,
          "curl": "curl -X PUT https://api.brillprime.com/api/auth/deactivate -H 'Authorization: Bearer YOUR_TOKEN'"
        }
      ]
    },
    "passwordReset": {
      "description": "Password reset functionality",
      "endpoints": [
        {
          "method": "POST",
