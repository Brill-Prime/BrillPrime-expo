    "orders": {
      "description": "Order management",
      "endpoints": [
        {
          "method": "POST",
          "path": "/api/orders",
          "description": "Create new order",
          "requiresAuth": true,
          "requestBody": {
            "merchantId": "string",
            "commodityId": "string",
            "quantity": "number",
            "deliveryAddress": "string",
            "deliveryType": "self|someone_else",
            "recipientName": "string (if deliveryType is someone_else)",
            "recipientPhone": "string (if deliveryType is someone_else)"
          },
          "curl": "curl -X POST https://api.brillprime.com/api/orders -H 'Authorization: Bearer YOUR_TOKEN' -H 'Content-Type: application/json' -d '{\"merchantId\":\"MERCHANT_ID\",\"commodityId\":\"COMMODITY_ID\",\"quantity\":2,\"deliveryAddress\":\"123 Main St\",\"deliveryType\":\"self\"}'"
        },
        {
          "method": "GET",
          "path": "/api/orders",
          "description": "Get user orders",
          "requiresAuth": true,
          "queryParams": {
            "status": "string (optional)",
            "limit": "number (optional)",
            "offset": "number (optional)"
          },
          "curl": "curl -X GET 'https://api.brillprime.com/api/orders?status=pending&limit=10' -H 'Authorization: Bearer YOUR_TOKEN'"
        },
        {
          "method": "GET",
          "path": "/api/orders/{orderId}",
