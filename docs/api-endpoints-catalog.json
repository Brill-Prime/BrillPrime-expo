          "curl": "curl -X GET 'https://api.brillprime.com/api/merchants/nearby?lat=9.0820&lng=8.6753&radius=10'"
        },
        {
          "method": "GET",
          "path": "/api/merchants/nearby/live",
          "description": "Get nearby merchants with live locations",
          "requiresAuth": true,
          "queryParams": {
            "lat": "number",
            "lng": "number",
            "radius": "number (km)"
