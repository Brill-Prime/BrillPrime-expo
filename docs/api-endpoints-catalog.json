          "requiresAuth": true,
          "requestBody": {
            "reason": "string (optional)"
          },
          "curl": "curl -X PUT https://api.brillprime.com/api/orders/ORDER_ID/cancel -H 'Authorization: Bearer YOUR_TOKEN' -H 'Content-Type: application/json' -d '{\"reason\":\"Changed my mind\"}'"
