          "requiresAuth": false,
          "curl": "curl -X GET https://api.brillprime.com/health"
        },
        {
          "method": "GET",
          "path": "/api/health",
          "description": "Basic health check",
          "requiresAuth": false,
          "curl": "curl -X GET https://api.brillprime.com/api/health"
        },
        {
          "method": "GET",
          "path": "/api/health/detailed",
          "description": "Detailed health check",
          "requiresAuth": false,
          "curl": "curl -X GET https://api.brillprime.com/api/health/detailed"
        }
      ]
