modules = ["nodejs-20", "postgresql-17", "nix", "typescript-language-server"]
[agent]
expertMode = true
integrations = ["javascript_database:1.0.0", "javascript_log_in_with_replit:1.0.0", "javascript_stripe:1.0.0", "javascript_sendgrid:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["supabase-cli"]



[deployment]
deploymentTarget = "autoscale"
run = ["sh", "-c", "npx expo start --web --port 5000 --host 0.0.0.0"]
build = ["npm", "run", "build"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Mobile Dev (LAN)"
author = 48462500
mode = "sequential"

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --lan --web"

[[workflows.workflow]]
name = "Start App"
author = 49172621
mode = "parallel"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Mobile Dev (LAN)"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --web --port 5000 --host 0.0.0.0 --clear"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Frontend"

[[workflows.workflow]]
name = "Frontend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run web"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

