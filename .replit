modules = ["nodejs-20"]
[agent]
expertMode = true
integrations = ["javascript_database:1.0.0", "javascript_log_in_with_replit:1.0.0", "javascript_stripe:1.0.0", "javascript_sendgrid:1.0.0"]

[nix]
channel = "stable-25_05"



[deployment]
deploymentTarget = "autoscale"
run = ["sh", "-c", "npx expo start --web --port 5000 --host 0.0.0.0"]
build = ["npm", "run", "build"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Start App"
author = 48040949
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --web --port 5000 --host 0.0.0.0"

[[workflows.workflow]]
