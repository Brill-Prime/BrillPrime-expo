modules = ["nodejs-20"]
[agent]
expertMode = true
integrations = ["javascript_database:1.0.0", "javascript_log_in_with_replit:1.0.0", "javascript_stripe:1.0.0", "javascript_sendgrid:1.0.0"]

[nix]
channel = "stable-25_05"



[deployment]
deploymentTarget = "autoscale"
run = ["npx", "serve", "dist", "-s", "-l", "5000"]
build = ["npm", "run", "build"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Frontend"

[[workflows.workflow]]
name = "Start App"
author = 48040949
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --web --port 5000"

[[workflows.workflow]]
name = "Frontend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run web"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3003

[[ports]]
localPort = 5002
externalPort = 4200

[[ports]]
localPort = 32799
externalPort = 5000

[[ports]]
localPort = 36805
externalPort = 3001

[[ports]]
localPort = 38123
externalPort = 3002

[[ports]]
localPort = 46753
externalPort = 3000
